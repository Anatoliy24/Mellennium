                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/softline_1.png" alt='')
                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/mts.png" alt='')
                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/adidas_1.png" alt='')
                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/oliwer_1.png" alt='')
                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/lilly_1.png" alt='')
                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/total_1.png" alt='')
                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/icopal_1.png" alt='')

              include _components/contacts
              include _components/map
              include _components/form
  include _components/scripts


