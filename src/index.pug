                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/fuji.png" alt='')
                        .section__slider-item
                          img.section__slider-logo(src="../../../assets/img/page_1/clients/hitachi.png" alt='')
